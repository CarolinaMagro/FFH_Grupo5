<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @*<title>@ViewData["Title"] - ProyectoFFHD</title>*@
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/bootstrap.min.js" rel="stylesheet" type="text/javascript" />
    <link href="~/Content/index.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/jquery-3.5.1.min.js" rel="stylesheet" type="text/javascript" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.8/css/solid.css" />
    <script src="https://use.fontawesome.com/releases/v5.0.7/js/all.js"></script>





    @*<script src="~/Scripts/modernizr-2.8.3.js"></script>*@
</head>

<body>

        <nav class="navbar">
            <div class="container">
                @Html.ActionLink("Far From Home", "Index", "FFH", new { area = "" }, new { @class = "navbar-brand texto-navegador" })
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>



            </div>
        </nav>

        <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        </div>

    <footer class="footer text-muted">
        <div class="container">
            &copy; 2020 - ProyectoFFH - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    @RenderSection("Scripts", required: false)
</body>
</html>
